<Window x:Class="Batch_Video_Converter.frmTitleSelect"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="frmTitleSelect" Height="335" Width="300"
        Loaded="OnLoaded" ResizeMode="NoResize">
    <Grid>
        <ListBox x:Name="lstTitles" HorizontalAlignment="Left" Height="173" Margin="25,49,0,0" VerticalAlignment="Top" Width="231"/>
        <Button x:Name="btnOK" Content="OK" HorizontalAlignment="Left" Margin="25,253,0,0" VerticalAlignment="Top" Width="75" Click="btnOK_Click"/>
        <Button x:Name="btnCancel" Content="Cancel" HorizontalAlignment="Left" Margin="181,253,0,0" VerticalAlignment="Top" Width="75" Click="btnCancel_Click"/>
        <Label x:Name="txtInputTitle" Content="" HorizontalAlignment="Left" Margin="25,20,0,0" VerticalAlignment="Top" Width="231"/>
    </Grid>
</Window>
